{
  "name": "agent-browser",
  "version": "1.0.0",
  "description": "Headless browser automation CLI for AI agents",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "agent-browser": "./bin/agent-browser"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "browser",
    "automation",
    "headless",
    "playwright",
    "cli",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "playwright": "^1.40.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.7.4",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  }
}
